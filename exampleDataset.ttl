##########################
exf:Feedback1
	rdf:type :Feedback;
	:submittedBy [
		foaf:accountName "user1"
	];
	:becauseOf	[ a :PassingReason, :TrafficJam],
				[ a :PermanentReason, :Pothole];

	:hasTimestamp [
		time:xsdDateTime "2020-11-24T11:09:35"^^xsd:dateTime ;
		:timeStampOf :Feedback1
	];
	:causedDelay [
		time:hasXSDDuration "PT7M"^^xsd:Duration
	];
	:atLocation [
		:locationOf :Feedback1;
		geo:long "7.020424";
		geo:lat "49.246158";
		:Address "St. Johann, 66123 Saarbrücken"^^xsd:string
	];
	:affects [
		a :Bus;
		:affectedBy :Feedback1;
		:Line "102"^^xsd:string
	] .

